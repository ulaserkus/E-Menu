{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AllowSwagger": true,
  "BaseUrl": "https://localhost:7030",
  "ConnectionStrings": {
    "MyCDSServer": "<cds-connectionString>"
  },
  "RateLimiting": {
    "UseForwardedHeader": false
  },
  "DirectLine": {
    "Url": "https://europe.directline.botframework.com/",
    "Secret": "<directline-clientSecret>"
  },
  "Quartz": {
    "CronSchedules": {
      "ImageSyncJob": "0 0 0 * * ?"
    }
  }
}
